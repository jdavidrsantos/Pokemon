{"ast":null,"code":"import PokemonPicture from '@/components/PokemonPicture.vue';\nimport PokemonOptions from '@/components/PokemonOptions.vue';\nimport getPokemonOptions from '@/helpers/getPokemonOptions'; // console.log(getPokemonOptions())\n\nexport default {\n  components: {\n    PokemonPicture,\n    PokemonOptions\n  },\n\n  data() {\n    return {\n      pokemonArr: [],\n      pokemon: '',\n      showPokemon: false\n    };\n  },\n\n  methods: {\n    async mixPokemonArray() {\n      this.pokemonArr = await getPokemonOptions();\n      console.log('pppppppp', this.pokemonArr); //Generar un numero random del 0 al 3\n\n      const rdnInt = Math.floor(Math.random() * 4);\n      console.log(rdnInt);\n      this.pokemon = this.pokemonArr[rdnInt];\n      console.log(this.pokemon);\n    },\n\n    checkAswer() {\n      console.log('pokemon page llamado');\n    }\n\n  },\n\n  mounted() {\n    this.mixPokemonArray();\n    console.log('soy pokemon arr', this.pokemonArr);\n  }\n\n};","map":{"version":3,"mappings":"AAaA,OAAOA,cAAP,MAA2B,iCAA3B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,iBAAP,MAA8B,6BAA9B,C,CAEA;;AAGA,eAAe;EACXC,UAAU,EAAE;IAAEH,cAAF;IAAkBC;EAAlB,CADD;;EAEXG,IAAI,GAAG;IACH,OAAO;MACHC,UAAU,EAAC,EADR;MAEHC,OAAO,EAAE,EAFN;MAGHC,WAAU,EAAI;IAHX,CAAP;EAKH,CARU;;EAUfC,OAAO,EAAC;IACL,MAAMC,eAAN,GAAuB;MAClB,KAAKJ,UAAL,GAAkB,MAAMH,iBAAiB,EAAzC;MACAQ,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAuB,KAAKN,UAA5B,EAFkB,CAK1B;;MACA,MAAMO,MAAK,GAAIC,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAc,CAAzB,CAAf;MACAL,OAAO,CAACC,GAAR,CAAYC,MAAZ;MACA,KAAKN,OAAL,GAAe,KAAKD,UAAL,CAAiBO,MAAjB,CAAf;MACCF,OAAO,CAACC,GAAR,CAAY,KAAKL,OAAjB;IAEI,CAZG;;IAcJU,UAAU,GAAE;MACTN,OAAO,CAACC,GAAR,CAAY,sBAAZ;IACH;;EAhBI,CAVO;;EAgCfM,OAAO,GAAE;IACL,KAAKR,eAAL;IACAC,OAAO,CAACC,GAAR,CAAa,iBAAb,EAA+B,KAAKN,UAApC;EACJ;;AAnCe,CAAf","names":["PokemonPicture","PokemonOptions","getPokemonOptions","components","data","pokemonArr","pokemon","showPokemon","methods","mixPokemonArray","console","log","rdnInt","Math","floor","random","checkAswer","mounted"],"sourceRoot":"","sources":["/Users/davidsantos/Documents/VUE-FERNANDO/04-pokemon-game/src/pages/PokemonPage.vue"],"sourcesContent":["<template>\n<h1 v-if=\"!pokemon\">Espere por favor...</h1>\n<div v-else>\n   <h1>Quien es este Pokemon?</h1>\n    <PokemonPicture :pokemonId = pokemon.id :showPokemon=\"showPokemon\"/>\n    <PokemonOptions :pokemons=\"pokemonArr\" \n    @selection=\"checkAswer()\"\n    />\n\n</div>  \n</template>\n\n<script>\nimport PokemonPicture from '@/components/PokemonPicture.vue';\nimport PokemonOptions from '@/components/PokemonOptions.vue';\nimport getPokemonOptions from '@/helpers/getPokemonOptions'\n\n// console.log(getPokemonOptions())\n\n\nexport default {\n    components: { PokemonPicture, PokemonOptions }, \n    data() {\n        return {\n            pokemonArr:[],\n            pokemon: '',\n            showPokemon : false\n        }\n    },\n\nmethods:{\n   async mixPokemonArray(){\n        this.pokemonArr = await getPokemonOptions()\n        console.log('pppppppp',this.pokemonArr)\n        \n\n//Generar un numero random del 0 al 3\nconst rdnInt = Math.floor(Math.random()*4)\nconsole.log(rdnInt)\nthis.pokemon = this.pokemonArr [rdnInt]\n console.log(this.pokemon)\n\n    },\n\n    checkAswer(){\n       console.log('pokemon page llamado') \n    }\n\n\n    \n},\n\nmounted(){\n    this.mixPokemonArray()\n    console.log( 'soy pokemon arr',this.pokemonArr)\n}\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}